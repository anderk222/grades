{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "nombre",
        "apellido",
        "maestro"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                0
            ]
        },
        "nombre": {
            "type": "string",
            "default": "",
            "title": "The nombre Schema",
            "examples": [
                "Ansufatiii"
            ]
        },
        "apellido": {
            "type": "string",
            "default": "",
            "title": "The apellido Schema",
            "examples": [
                "Br"
            ]
        },
        "maestro": {
            "type": "object",
            "default": {},
            "title": "The maestro Schema",
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The id Schema",
                    "examples": [
                        1
                    ]
                }
            },
            "examples": [{
                "id": 1
            }]
        }
    },
    "examples": [{
        "id": 0,
        "nombre": "Ansufatiii",
        "apellido": "Br",
        "maestro": {
            "id": 1
        }
    }]
}