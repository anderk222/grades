{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "materia",
        "alumno",
        "puntos"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                0
            ]
        },
        "materia": {
            "type": "object",
            "default": {},
            "title": "The materia Schema",
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The id Schema",
                    "examples": [
                        1
                    ]
                }
            },
            "examples": [{
                "id": 1
            }]
        },
        "alumno": {
            "type": "object",
            "default": {},
            "title": "The alumno Schema",
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The id Schema",
                    "examples": [
                        1
                    ]
                }
            },
            "examples": [{
                "id": 1
            }]
        },
        "puntos": {
            "type": "number",
            "default": 0.0,
            "title": "The puntos Schema",
            "examples": [
                5.45
            ]
        }
    },
    "examples": [{
        "id": 0,
        "materia": {
            "id": 1
        },
        "alumno": {
            "id": 1
        },
        "puntos": 5.45
    }]
}